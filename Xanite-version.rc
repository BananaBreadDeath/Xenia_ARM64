      VALUE "LegalTrademarks",      "Vulkan 1.1, OpenGL 3.0, Android 10"
      VALUE "OriginalFilename",     "Xanite"
      VALUE "SpecialNote",          "This file is part of the Xbox Original core only."
    END
  END

  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 1200
  END
END

IDI_ISO "GAME.ISO"
